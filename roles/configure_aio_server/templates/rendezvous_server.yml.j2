    path: /etc/fdo/aio/stores/rendezvous_registered
session_store_driver:
  Directory:
    path: /etc/fdo/aio/stores/rendezvous_sessions
trusted_manufacturer_keys_path: /etc/fdo/aio/keys/manufacturer_cert.pem
max_wait_seconds: ~
bind: "0.0.0.0:{{ rendezvous_server_port }}"